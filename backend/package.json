{
  "name": "user-api-server",
  "version": "1.0.0",
  "description": "Express.js API server with CRUD operations for User model using PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-data": "node scripts/initData.js",
    "clear-data": "node -e \"require('./scripts/initData').clearData().then(() => process.exit(0)).catch(() => process.exit(1))\"",
    "reset-data": "node -e \"require('./scripts/initData').resetData().then(() => process.exit(0)).catch(() => process.exit(1))\"",
    "test-init": "node scripts/testInit.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["express", "api", "postgresql", "crud", "user"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}